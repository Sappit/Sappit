{
  "name": "sappit",
  "description": "Sappit - Reddit Client written with Svelte Sapper",
  "tags": [
    "reddit",
    "sapper",
    "svelte"
  ],
  "version": "0.0.1",
  "scripts": {
    "dev": "PORT=10080 sapper dev",
    "build": "sapper build",
    "export": "sapper export && netlify-lambda build ./functions/",
    "lambda": "netlify-lambda serve .",
    "lint": "eslint src/ --ext svelte",
    "sass": "node-sass --output-style=compressed --source-map=true src/sass/unorganized.sass static/css/unorganized.css",
    "sass:watch": "nodemon --watch src/sass/unorganized.sass --exec \"node-sass --output-style=compressed --source-map=true src/sass/unorganized.sass static/css/unorganized.css\"",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@sentry/browser": "^5.9.1",
    "axios": "0.18",
    "axios-retry": "^3.1.2",
    "brace": "^0.11.1",
    "compression": "^1.7.1",
    "date-fns": "1",
    "dotenv": "^8.2.0",
    "http-proxy-middleware": "^0.20.0",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.15",
    "mark.js": "^8.11.1",
    "mini-css-extract-plugin": "^0.8.0",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "polka": "next",
    "prettier-plugin-svelte": "^0.7.0",
    "qs": "^6.9.0",
    "sass-loader": "^8.0.0",
    "sirv": "^0.4.0",
    "sql-formatter": "^2.3.3",
    "svelte-ace-editor": "^0.0.1",
    "svelte-backtotop": "^0.0.0",
    "svelte-preprocess": "^3.2.0-alpha.2",
    "tumblr.js": "^2.0.2"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.9.3",
    "eslint": "^6.6.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.14.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  }
}
